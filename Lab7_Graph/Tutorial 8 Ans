class Vertex<T extends Comparable<T>, N extends Comparable <N>> {
    T vertexInfro;
    int indeg;
    int outdeg;
    Vertex<T,N> nextVertex;
    Edge<T,N> firstEdge;

    public Vertex() {
        
    }
}
